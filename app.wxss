/* pulbic -- start */
.container{
  position: relative;
  width: 100%;
  height: 100%;
}
.content{
  padding: 0 30rpx;
  text-align: justify;
}
.scroll-box{
  position: absolute;
  width: 100%;
  height: 100%;
}
.hidden {
  display:none;
}

button {
  border:none;
}
button:after {
  border:none;
}

.pa-0 { padding:0; }
.pa-10{ padding: 10rpx; }
.pt-10{ padding-top: 10rpx!important; }
.pr-10{ padding-right: 10rpx; }
.pb-10{ padding-bottom: 10rpx; }
.pl-10{ padding-left: 10rpx; }
.pl-40{ padding-left: 40rpx; }
.pa-20{ padding: 20rpx; }
