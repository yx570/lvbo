.panel-cart-fill{
    min-height: 100%;
    padding-bottom: 140rpx;
    overflow: hidden;
    background: #f2f2f2;
}
.panel-cart-fill .at-footer{
    background: #fff;
}
.panel-cart-fill .at-footer .left text{
    font-size: 46rpx;
}
.panel-cart-fill .at-footer .left text.sub{
    font-size: 28rpx;
}
.panel-cart-fill .weui-cells{
    margin-top: 20rpx;
}
.panel-cart-fill .fill-address{
    margin-top: 0;
}
.order-item image{
    display: inline-block;
    width: 160rpx;
    height: 160rpx;
    vertical-align: middle;
}
.order-item .weui-cell__bd{
    padding-left: 20rpx;
}

.order-item .name{
    padding-bottom: 26rpx;
    font-size: 32rpx;
    color: #333;
}
.order-item .price{
    padding-top: 26rpx;
    font-size: 34rpx;
}
.order-item .price .sub{
    font-size: 28rpx;
}
.order-item .price .color-gray{
    padding-left: 20rpx;
    font-size: 28rpx;
}
.fill-address::before,.fill-address::after{
    height: 0;
    border:0;
}
.fill-address{
    position: relative;
}
.fill-address .line{
    width: 100%;
    font-size: 0;
    overflow: hidden;
}
.fill-address .line image{
    width: 100%;
    height: 10rpx;
    line-height: 1;
}
.fill-bottom .weui-cell__ft{
    color: #333;
}
.wc{
  width: 480rpx;
  margin-top: 20rpx ;
  margin-bottom: 20rpx;
  height: 70rpx;
  font-size: 32rpx;
  line-height: 70rpx;
}
.a{
  background: #fff;
  width: 100%;
  border-radius: 20rpx;
  position: absolute;
  bottom: 90rpx;    
}