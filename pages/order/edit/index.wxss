.stepper {
  width:200rpx;
  height:70rpx  !important;
  line-height: 70rpx;
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
  color: #333;
}
/*加号和减号*/
.stepper text {
  float: left;
  width: 50rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
}
/*数值*/
.stepper input {
  width: 90rpx;
  float: left;
  margin: 0 auto;
  text-align: center;
  font-size: 35rpx;
  height: 70rpx;
  border-left: 1rpx solid #ccc;
  border-right: 1rpx solid #ccc;
}
/*普通样式*/
.stepper .normal {
  color:black;
}
/*禁用样式*/
.stepper .disable {
  color: #333;
} 
/* 奇数行  */
.vwdataodd{ 
