page { background-color: #f3f3f3;}
.panel-orderDetails .header{
  position: relative;
  height:250rpx;
}
.panel-orderDetails .infos {
  position: absolute;
  left:30rpx;
  right:30rpx;
  top:145rpx;
  background-color:#fff;
  border-radius:30rpx;
}
.panel-orderDetails .infos .img-box{
  width: 180rpx;
  height: 180rpx;
  border-radius: 30rpx;
  overflow:hidden;  
}
.panel-orderDetails .priceBox { font-size: 34rpx;}
.panel-orderDetails .priceBox text { font-size: 20rpx;}
.panel-orderDetails .title{
  padding: 0;
  font-size: 26rpx;
}
.panel-orderDetails .sum{
  padding: 10rpx 0;
  max-height: 80rpx;
  overflow:hidden;  
}
.panel-orderDetails .sum text {
  margin-left:50rpx;
}
.panel-orderDetails .status{
  line-height: 2;
}
.panel-orderDetails .infos .app-row { padding: 30rpx;}
.panel-orderDetails .app-rows { padding:0;}
.panel-orderDetails .list { margin-top:174rpx;}
.panel-orderDetails .list .app-row { margin:0 30rpx 30rpx; padding: 18rpx 30rpx 2rpx; background-color:#fff; border-radius:30rpx;}
.panel-orderDetails .list .img-box{
  margin: 0 26rpx;
  border-radius: 50%;
}
.panel-orderDetails .list image{
  border-radius: 50%;
}
.panel-orderDetails .list .date{
  padding: 26rpx 0;
}
.panel-orderDetails .list .iconfont{
  font-size: 50rpx;
  line-height: 1;
}
.panel-orderDetails .orderStatus { 
  position: absolute;
  left:30rpx;
  top:60rpx;
  font-size:36rpx;
  color:#fff;
}
.panel-orderDetails .orderStatus .iconfont { color:#fff; margin-right:20rpx;}

.panel-orderDetails .list .app-row.finish { background-color: #e7e7e7; border:1rpx solid #dadada; color:#b3b3b3;}
.panel-orderDetails .list .finish .title { color:#b3b3b3;}
.panel-orderDetails .list .title .flex-bd { font-size: 32rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;}

.panel-orderDetails .list .flex-row { height:100rpx; line-height: 100rpx; margin:0;}
.panel-orderDetails .list .items { border-top:1rpx solid #f3f3f3;} 
.panel-orderDetails .list .finish .items { border-top:1rpx solid #dadada;} 
.listHeadPic .img-box { width:65rpx; height:65rpx;}
.footName { position: relative; top:4rpx; margin-right:20rpx;}
.technicianBox { padding:0 0 40rpx 0; box-sizing: content-box; line-height:1.2; border-top:none!important;}
.technicianBox .img-box { width:100rpx; height:100rpx; margin:0 30rpx 0 0!important;}
.technicianBox .name { padding-top:10rpx; font-size: 30rpx; line-height: 1.2}
.panel-orderDetails .stars { margin-top:10rpx; line-height: 1.2}
.panel-orderDetails .stars .iconfont { font-size: 32rpx; color:#ffcc00; margin-right:5rpx;}
.technicianBox .dates { padding-top:10rpx; line-height: 1.5; font-size: 28rpx;}
.technicianBox .times { font-size: 30rpx; line-height: 1.5;}

.panel-orderDetails .list .icon-weizhi { font-size: 40rpx; color:#333;}
.panel-orderDetails .list .icon-arrow-right { font-size: 30rpx; color:#c9c9c9;}
.panel-orderDetails .countTime { color:#f9879a;}
.panel-orderDetails .countTime text { background-color: #f9879a; padding:5rpx 6rpx; margin:2rpx; border-radius: 10rpx; color:#fff;}

.beginBtn { width:104rpx; height:104rpx; line-height:102rpx; font-size:30rpx; background:#fff; border:1rpx solid #00b0ab; border-radius: 50%; box-sizing: border-box; color:#00b0ab; padding:0;}